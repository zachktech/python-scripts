c=db.cursor()
c2=db.cursor()
c.execute('SHOW tables from fullcast2')
tables=c.fetchall()
for table in tables:
	#print(table)
	#print(len(table))
	print(table[0])
	temp=table[0]
	concate= "DESCRIBE " + temp + " from fullcast2"
	c2.execute('DESCRIBE Acctswtransactionandcompany from fullcast2')
	columns=c2.fetchall()
	for column in columns:
		print(column[0])

